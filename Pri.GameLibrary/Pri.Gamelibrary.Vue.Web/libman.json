{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "cdnjs",
      "library": "vue@2.7.15",
      "destination": "wwwroot/vue/",
      "files": [
        "vue.js"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "axios@1.6.3",
      "destination": "wwwroot/axios/"
    },
    {
      "provider": "unpkg",
      "library": "jwt-decode@3.1.2",
      "destination": "wwwroot/lib/jwt-decode/",
      "files": [
        "build/jwt-decode.js"
      ]
    }
  ]
}